{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "clean and package",
            "type": "shell",
            "command": "mvn clean package",
            "group": {
                "kind": "build",
                "isDefault": true
            }
        },
        {
            "label": "package only",
            "type": "shell",
            "command": "mvn package",
            "group": {
                "kind": "build",
                "isDefault": true
            }
        },
    ]
}